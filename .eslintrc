{
  "extends": [
    "wesbos"
  ],
  "rules": {
    "no-console": 0,
    "no-plusplus": 0,
    "eqeqeq": 0,
    "prefer-const": 0,
    "array-callback-return": 0,
    "no-shadow": 0,
    "no-param-reassign": 0,
    "no-loop-func": 0,
    "no-use-before-define": 0,
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 160,
        "tabWidth": 2,
        "semi": false,
      }
    ]
  }
}